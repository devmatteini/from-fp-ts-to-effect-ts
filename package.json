{
  "name": "from-fp-ts-to-effect-ts",
  "version": "1.0.0",
  "description": "An exploration of EffectTS ecosystem from fp-ts user prospective",
  "scripts": {
    "format": "prettier --write src/**/*.ts",
    "test": "vitest run --passWithNoTests",
    "test:w": "vitest watch",
    "typecheck": "tsc --noEmit",
    "typecheck:w": "tsc --noEmit --watch"
  },
  "dependencies": {
    "effect": "2.4.19",
    "@effect/schema": "0.65.0",
    "fp-ts": "2.16.1",
    "io-ts": "2.2.20",
    "fp-ts-contrib": "0.1.29",
    "retry-ts": "0.1.4",
    "node-fetch": "3.3.2"
  },
  "devDependencies": {
    "@types/node": "16.18.12",
    "typescript": "5.2.2",
    "vitest": "0.34.6",
    "prettier": "3.1.0",
    "tsx": "4.1.2"
  }
}
