{
  "name": "from-fp-ts-to-effect-ts",
  "version": "1.0.0",
  "description": "An exploration of EffectTS ecosystem from fp-ts user prospective",
  "scripts": {
    "format": "prettier --write src/**/*.ts",
    "test": "vitest run",
    "test:w": "vitest watch",
    "typecheck": "tsc --noEmit",
    "typecheck:w": "tsc --noEmit --watch"
  },
  "dependencies": {
    "@effect/io": "0.38.2",
    "@effect/data": "0.17.6",
    "@effect/schema": "0.33.2",
    "fp-ts": "2.13.1",
    "io-ts": "2.2.20",
    "fp-ts-contrib": "0.1.29",
    "retry-ts": "0.1.4",
    "node-fetch": "3.3.0"
  },
  "devDependencies": {
    "@types/node": "16.18.12",
    "typescript": "5.1.6",
    "vitest": "0.28.5",
    "prettier": "2.8.4",
    "tsx": "3.12.3"
  }
}
